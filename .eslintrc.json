{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "no-underscore-dangle": 0,
    "linebreak-style": 0,
    "global-require": 0,
    "no-await-in-loop": 0,
    "no-console": 0
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
